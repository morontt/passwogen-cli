{
    "directories": ["src/"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "vendor"
        }
    ],
    "files": [
        "LICENSE"
    ],
    "compression": "GZ",
    "git-version": "package_version",
    "main": "bin/script.php",
    "output": "passwogen.phar",
    "stub": true,
    "chmod": "0755"
}
